.DrawType {
  width: 100%;
  display: flex;
  height: 593rpx;
  flex-direction: column;
}

.TopView {
  width: 100%;
  display: flex;
  height: 140rpx;
}

.TopView .TitleImage {
  width: 100%;
  height: 593rpx;
}

.ViewType {
  width: 100%;
  display: flex;
  height: 400rpx;
  flex-direction: column;
  align-items: center;
}

.TextView1 {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 180rpx;
}

.TextView1 .text1 {
  font-size: 40rpx;
  color: #fff;
  font-family: "微软雅黑";
  line-height: 60rpx;
  margin-right: 70rpx;
}

.TextView1 .text2 {
  font-size: 70rpx;
  color: #fdd300;
  font-family: "微软雅黑";
  line-height: 100rpx;
  margin-right: 70rpx;
}

.TextView2 {
  height: 60rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  align-content: center;
  margin-right: 70rpx; 
  width: 100%;
}

.TextView2 .text1 {
  font-size: 40rpx;
  color: #fff;
  font-family: "微软雅黑";
  line-height: 60rpx;
}

.TextView2 .text2 {
  font-size: 40rpx;
  color: #fed300;
  font-family: "微软雅黑";
  line-height: 60rpx;
}

.ViewRecordTitle {
  height: 163rpx;
  display: flex;
  width: 100%;
  flex-direction: column;
  align-items: center;
}

.ViewRecordTitle .image {
  width: 690rpx;
  height: 163rpx;
}

.ItemListView {
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.ItemView {
  width: 690rpx;
  height: 160rpx;
  border-top: 1rpx solid #9fd7e9;
  display: flex;
  justify-content: flex-start;
  padding-top: 5rpx;
}

.ItemView .image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  margin-left: 100rpx;
}

.ItemView .text {
  font-size: 60rpx;
  line-height: 150rpx;
  font-family: "微软雅黑";
  display: block;
  width: 300rpx;
}

.ItemView .text2 {
  font-size: 30rpx;
  line-height: 150rpx;
  color: #fb5689;
  font-family: "微软雅黑";
  margin-right: 20rpx;
}

.ViewBottom {
  height: 39rpx;
  display: flex;
  width: 100%;
  justify-content: center;
  margin-bottom: 50rpx;
}

.ViewBottom .image {
  width: 690rpx;
  height: 39rpx;
}
